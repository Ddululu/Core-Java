
public class DoDemo {

	public static void main(String[] args) {
		// Do 문을 이용한 Factorial
		System.out.print("숫자를 입력하세요: ");
		java.util.Scanner sc = new java.util.Scanner(System.in);
		int num = sc.nextInt();
		int i=1;
		int result;
		do{
			result= result*i;
			System.out.printf("결과는 %d ",num);
		}while(i< num);
		}
	}

}
