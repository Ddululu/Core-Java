

public class VariableDemo {
	public static void main(String[] args) {
//		String name; // 변수 선언
//		name ="황신욱"; //name에 값을 할당
		int a = 0xFFFFFFFF;
		byte b = 0x10;
		System.out.print('A');
		System.out.print('A');
		System.out.print('\n');
		System.out.print('A');
		
		int A= 10;
		System.out.print('\b');
		System.out.print('B');
		
		
	}
}
